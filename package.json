  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "props|state",
          "ignoreRestSiblings": true,
          "args": "none"
        }
      ],
      "no-return-assign": [
        "off"
      ]
    }
  },
  "standardx": {
    "globals": [
      "fetch"
    ],
    "ignore": [
      "/plugins",
      "/static"
    ]
  }
}
